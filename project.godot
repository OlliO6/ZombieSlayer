; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "RichTextEffect",
"class": "TintTextEffect",
"language": "GDScript",
"path": "res://addons/DebugOverlay/RichTextColorEfx.gd"
} ]
_global_script_class_icons={
"TintTextEffect": ""
}

[Diadot]

SkipInput="SkipDialog"
DefaultDelay=0.06
CharsToSkipWhenSkippPressed=8

[MonoCustomResourceRegistry]

ResourceScriptDirectories=[ "res://" ]
SearchType=0
ClassPrefix=""

[application]

config/name="ZombieSlayer"
run/main_scene="res://BootSplash/BootSplash.tscn"
boot_splash/image="res://BootSplash/SplashScreen.png"
boot_splash/use_filter=false
config/icon="res://ZombieIcon.png"

[audio]

default_bus_layout="res://BusLayout.tres"

[autoload]

InputManager="*res://Singletons/InputManager.cs"
OptionsManager="*res://Singletons/Options/OptionsManager.cs"
Debug="*res://addons/DebugOverlay/DebugOverlay.tscn"
ExplanationsManager="*res://Singletons/Explanations/Explanations.tscn"
Database="*res://Singletons/Database.cs"
Transitions="*res://Singletons/Transitions/Transitions.tscn"
UiManager="*res://Singletons/UI/UIManager.tscn"

[display]

window/size/width=256
window/size/height=144
window/size/test_width=1280
window/size/test_height=720
window/dpi/allow_hidpi=true
mouse_cursor/custom_image="res://UI/Theme/MouseCursor.png"
mouse_cursor/tooltip_position_offset=Vector2( 1, 1 )
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/BetterFileSystem/plugin.cfg", "res://addons/DebugOverlay/plugin.cfg", "res://addons/Diadot/plugin.cfg", "res://addons/MonoCustomResourceRegistry/plugin.cfg", "res://addons/Random Scene Node/plugin.cfg", "res://addons/Shake/plugin.cfg", "res://addons/imrp/plugin.cfg", "res://addons/kanban_tasks/plugin.cfg", "res://addons/project-statistics/plugin.cfg" )

[gui]

theme/custom="res://UI/Theme/Theme.tres"
timers/tooltip_delay_sec=0.4
common/snap_controls_to_pixels=false

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}
wav={
"compress/mode": 0,
"edit/loop": false,
"edit/normalize": false,
"edit/trim": false,
"force/8_bit": false,
"force/max_rate": false,
"force/max_rate_hz": 44100,
"force/mono": true
}

