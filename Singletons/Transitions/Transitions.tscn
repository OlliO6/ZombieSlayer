[sub_resource type="Shader" id=5]
code = "shader_type canvas_item;

uniform float progress;
uniform float lenght = 0.1;

void fragment() {
	vec2 m_uv = (UV - vec2(0.5)) * 2.0;
	float a = smoothstep(progress, progress + lenght, abs(m_uv.x));
	COLOR.a = a;
}
"

