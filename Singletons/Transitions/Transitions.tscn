[sub_resource type="Shader" id=9]
code = "shader_type canvas_item;

uniform float progress;
uniform float step_size = 0.1;
uniform sampler2D noise;

void fragment() {
	float stepped = round(progress / step_size) * step_size;
	float a = progress + texture(noise, UV + vec2(stepped * 16.0)).r;
	COLOR.a = clamp(a, 0., 1.);
}
"

